{
    "manifest_version": 2,
    "name": "Search Rank",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_title": "SearchRank",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "declarativeContent",
        "storage"
    ],
    "web_accessible_resources": [
      "jquery.min.js",
      "inject.js",
      "inject.css",
      "upvote.png",
      "downvote.png"],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["jquery.min.js", "index.js"]
    }]
}
